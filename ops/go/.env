API_ENV=dev
API_PORT=8080
API_FRONT_URL=https://localhost:3443
API_APP_DOMAIN=http://localhost:3010/

# Server Configuration
GIN_MODE=debug # debug, release, test

# Database Configuration
DB_HOST=mysql
DB_PORT=3306
DB_USER=root
DB_PASSWORD=rootpw
